<Window x:Class="QControlServer.SetInfoDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QControlServer"
        mc:Ignorable="d"
        Title="设置客户机器的信息" Height="220" Width="300" ResizeMode="NoResize">
    <StackPanel>
        <TextBox x:Name="MainBox" HorizontalAlignment="Left" Height="45" Margin="50,20,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="206"/>
        <Button x:Name="Confirm" Content="确定" HorizontalAlignment="Left" Margin="108,20,0,0" VerticalAlignment="Top" Width="75" Click="Confirm_Click"/>
        <Button x:Name="Cancel" Content="取消" HorizontalAlignment="Left" Margin="108,20,0,0" VerticalAlignment="Top" Width="75" Click="Cancel_Click"/>
    </StackPanel>
</Window>
